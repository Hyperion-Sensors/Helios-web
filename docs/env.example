##############################
# Server-side (never expose)
##############################

# NextAuth
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=replace-with-strong-random-string

# AWS Cognito (NextAuth provider)
COGNITO_CLIENT_ID=
COGNITO_CLIENT_SECRET=
COGNITO_ISSUER=
# Optional: domain for sign-out flow
COGNITO_DOMAIN=

# Database connection (used server-side in pages like overview/stats)
DB_HOST=

##############################
# Client-side (NEXT_PUBLIC_*)
##############################

# API base URL your frontend calls
NEXT_PUBLIC_SERVER_ADDR=http://localhost:4000

# Mapbox
NEXT_PUBLIC_MAPBOX_TOKEN=

# Misc client env selector used by visualizer models
NEXT_PUBLIC_CLIENT_ENV=local

##############################
# Bug report S3 (server-side)
##############################
S3_BUG_REPORTS_BUCKET=
S3_BUG_REPORTS_REGION=
S3_BUG_REPORTS_PREFIX=complaints


